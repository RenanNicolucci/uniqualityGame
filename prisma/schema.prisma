generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id      Int       @id @default(autoincrement())
  name    String
  Answers Answers[]
  time    String
  createdAt DateTime @default(now())
}

model Answers {
  id       Int     @id @default(autoincrement())
  key      String
  quantity Int
  product  Int
  userId   Int
  user     User    @relation(fields: [userId], references: [id])
  wrong    Boolean
}
