generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Answers {
  id       Int    @id @default(autoincrement())
  key      String
  quantity Int
  product  Int
}
